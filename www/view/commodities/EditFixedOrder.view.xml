									<!--Data Pagamento 3-->
									<Label text="{i18n>textPaymentDate} 3" design="Standard" required="false" textAlign="Begin" textDirection="Inherit" visible="{fixedOrderFormModel>/visibleFixedDate}"/>
									<DatePicker id="idDateInput3" placeholder="{i18n>placeSelectPaymentDate1}" displayFormat="short" name="HCP_ZDTPGTO3" enabled="{fixedOrderFormModel>/enabled}"
										value="{ path: 'fixedOrderFormModel>/HCP_ZDTPGTO3', type: 'sap.ui.model.type.Date', formatOptions: { style: 'short', UTC: false, pattern: 'dd/MM/yyyy' } }"
										dateValue="" change="_onInputPaymentDateFormSelect" visible="{fixedOrderFormModel>/visibleFixedDate}"/>
