<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:form="sap.ui.layout.form"
	xmlns:custom="com.sap.build.standard.brfAppDeGraosModoEditar.controls">
	<ResponsivePopover title="Filtro Contato Simplificado" placement="Bottom">
		<content>
			<form:Form width="100%" editable="true" visible="true">
				<form:title/>
				<form:formContainers>
					<form:FormContainer visible="true">
						<form:formElements>
							<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n>textCommercializationId}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input placeholder="{filtersCommercialization>/HCP_COMMERCIALIZATION_ID}" type="Text" showValueHelp="false" enabled="true" visible="true" width="auto"
										valueHelpOnly="false" maxLength="0" value="{filtersCommercialization>/HCP_COMMERCIALIZATION_ID}"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n>textVendorOrProspect}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input placeholder="{filtersCommercialization>/HCP_PARTNER}" type="Text" showValueHelp="false" enabled="true" visible="true" width="auto"
										valueHelpOnly="false" maxLength="0" value="{filtersCommercialization>/HCP_PARTNER}"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n>textCommercializationType}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input placeholder="{filtersCommercialization>/HCP_TYPE_COMMERCIALIZATION}" type="Text" showValueHelp="false" enabled="true" visible="true" width="auto"
										valueHelpOnly="false" maxLength="0" value="{filtersCommercialization>/HCP_TYPE_COMMERCIALIZATION}"/>
								</form:fields>
							</form:FormElement>
							
							<form:FormElement visible="true">
								<form:label>
									<Label text="Data de" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<DatePicker placeholder="Dia / Mês / Ano" displayFormat="short"
										value="{ path: 'filtersCommercialization>/HCP_START_DATE', type: 'sap.ui.model.type.Date', formatOptions: { style: 'short', UTC: true, pattern: 'dd/MM/yyyy' } }"
										dateValue="" change="test"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Data Até" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<DatePicker placeholder="Dia / Mês / Ano" displayFormat="short"
										value="{ path: 'filtersCommercialization>/HCP_END_DATE', type: 'sap.ui.model.type.Date', formatOptions: { style: 'short', UTC: true, pattern: 'dd/MM/yyyy' } }"
										dateValue=""/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Usuário" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input placeholder="{filtersCommercialization>/HCP_CREATED_BY}" type="Text" showValueHelp="false" enabled="true" visible="true" width="auto"
										valueHelpOnly="false" maxLength="0" value="{filtersCommercialization>/HCP_CREATED_BY}"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:fields>
									<Button text="{i18n>btnsearch}" type="Emphasized" press="submitFilterListCommercialization"/>
								</form:fields>
							</form:FormElement>
						</form:formElements>
						<form:title/>
					</form:FormContainer>
				</form:formContainers>
				<form:layout>
					<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
				</form:layout>
			</form:Form>
		</content>
	</ResponsivePopover>
</core:FragmentDefinition>