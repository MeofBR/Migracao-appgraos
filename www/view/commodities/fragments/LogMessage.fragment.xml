<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:form="sap.ui.layout.form"
	xmlns:custom="com.sap.build.standard.brfAppDeGraosModoEditar.controls">
	<Dialog showHeader="true" title="{i18n>textLogMessage}" type="Standard">
		<content>
			<Panel width="auto">
				<form:SimpleForm class="simpleFormCommerc" id="cadenceKeysForm" editable="true" maxContainerCols="1">
					<form:content>
						<Table width="auto" showSeparators="All" class="sapUiResponsiveMargin" id="table" items="{ path:'messageLogFormModel>/tableMessage'}">
							<infoToolbar>
								<Toolbar width="100%" height="auto" design="Auto" visible="false" enabled="true">
									<content>
										<Label text="Label" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
									</content>
								</Toolbar>
							</infoToolbar>
							<columns>
								<Column width="50px" hAlign="Left" vAlign="Top" minScreenWidth="Phone" demandPopin="false" popinDisplay="Inline" mergeDuplicates="false">
									<header>
										<Text text="{i18n>textType}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
									</header>
									<footer/>
								</Column>
								<Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
									<header>
										<Text text="{i18n>textMessage}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
									</header>
									<footer/>
								</Column>
							</columns>
							<items>
								<ColumnListItem type="Active">
									<cells>
										<core:Icon size="2rem" src="{ path: 'messageLogFormModel>ICON'}"/>
										<ObjectIdentifier titleActive="false" visible="true" title="{ path: 'messageLogFormModel>HCP_MESSAGE'}"/>
									</cells>
								</ColumnListItem>
							</items>
						</Table>
					</form:content>
				</form:SimpleForm>
			</Panel>
		</content>
		<buttons>
			<Button text="{i18n>buttonConfirm}" press="_onMsgLogConfirPress" type="Emphasized" iconFirst="true" width="auto" enabled="true"
				visible="true" iconDensityAware="false"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>