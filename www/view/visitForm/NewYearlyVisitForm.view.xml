<mvc:View xmlns:mvc="sap.ui.core.mvc"
	controllerName="com.sap.build.standard.brfAppDeGraosModoEditar.controller.visitForm.NewYearlyVisitForm" xmlns:uxap="sap.uxap" xmlns="sap.m"
	xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:semantic="sap.m.semantic" xmlns:u="sap.ui.unified"
	xmlns:custom="com.sap.build.standard.brfAppDeGraosModoEditar.controls">
	<uxap:ObjectPageLayout showTitleInHeaderContent="true" showHeaderContent="true" showAnchorBar="true" showFooter="true"
		subSectionLayout="TitleOnTop">
		<uxap:headerTitle>
			<uxap:ObjectPageDynamicHeaderTitle class="displayArrow">
				<uxap:breadcrumbs>
					<OverflowToolbarButton type="Transparent" icon="sap-icon://nav-back" tooltip="copy" press="onCancelPress"/>
				</uxap:breadcrumbs>
				<uxap:expandedHeading>
					<FlexBox wrap="Wrap" fitContainer="true" alignItems="Center">
						<Title text="Ficha de Visitas Anual" wrapping="true" class="sapUiTinyMarginEnd sapUiTinyMarginTop"/>
					</FlexBox>
				</uxap:expandedHeading>
				<uxap:expandedContent>
					<FlexBox wrap="Wrap" fitContainer="true" alignItems="Center">
						<Title
							text="{i18n>textVendorProspect}: {yearlyVisitModel>/HCP_PROVIDER_ID} - {yearlyVisitModel>/HCP_NAME_REGISTERED}"
							width="100%" textAlign="Begin" textDirection="Inherit" wrapping="true"/>
					</FlexBox>
				</uxap:expandedContent>
				<uxap:snappedContent>
					<FlexBox wrap="Wrap" fitContainer="true" alignItems="Center">
						<Title
							text="{i18n>textVendorProspect}: {yearlyVisitModel>/HCP_PROVIDER_ID} - {yearlyVisitModel>/HCP_NAME_REGISTERED}"
							width="100%" textAlign="Begin" textDirection="Inherit" wrapping="true"/>
					</FlexBox>
				</uxap:snappedContent>
				<uxap:navigationActions>
					<Button text="Salvar" type="Emphasized" iconFirst="true" width="auto" enabled="{yearlyVisitModel>/enableSave}" visible="true"
						iconDensityAware="false" press="onSavePress"/>
				</uxap:navigationActions>
			</uxap:ObjectPageDynamicHeaderTitle>
		</uxap:headerTitle>
		<uxap:sections>
			<uxap:ObjectPageSection showTitle="true" title="Cadastro" titleUppercase="true" visible="true">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="Cadastro" mode="Expanded">
						<uxap:blocks>
							<VBox>
								<form:SimpleForm class="simpleForm" columnsL="1" columnsM="1" columnsXL="1" editable="true" emptySpanL="4" emptySpanM="4" emptySpanXL="4"
									id="newEntitySimpleForm" labelSpanL="3" labelSpanM="3" labelSpanXL="3" layout="ResponsiveGridLayout" maxContainerCols="2" minWidth="2048">
									<form:content>
										<Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<form:SimpleForm class="simpleForm" width="100%" columnsL="1" columnsXL="1" columnsM="1" editable="true" emptySpanL="4" emptySpanM="0"
											emptySpanXL="0" id="ufPlantingDataSimpleForm" labelSpanL="3" labelSpanM="3" labelSpanXL="3" layout="ResponsiveGridLayout"
											maxContainerCols="2" minWidth="2048">
											<form:content></form:content>
										</form:SimpleForm>
										<Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<Button text="Adicionar Novo UF Plantio" type="Emphasized" iconFirst="true" enabled="true" visible="true" iconDensityAware="false"
											press="_onAddNewForm">
											<customData>
												<core:CustomData key="id" value="ufPlantingDataSimpleForm"/>
											</customData>
											<layoutData>
												<l:GridData span="L5 M5 S12"/>
											</layoutData>
										</Button>
										<Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<form:SimpleForm class="simpleForm" width="100%" columnsL="1" columnsM="1" columnsXL="1" editable="true" emptySpanL="4" emptySpanM="0"
											emptySpanXL="0" id="newCultureSimpleForm" labelSpanL="3" labelSpanM="3" labelSpanXL="3" layout="ResponsiveGridLayout" maxContainerCols="2"
											minWidth="2048">
											<form:content></form:content>
										</form:SimpleForm>
										<Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<Button class="sapUiLargeMarginBottom" id="cultureFormID" text="Adicionar Nova Cultura" type="Emphasized" iconFirst="true" enabled="true"
											visible="true" iconDensityAware="false" press="_onAddNewForm">
											<customData>
												<core:CustomData key="id" value="newCultureSimpleForm"/>
											</customData>
											<layoutData>
												<l:GridData span="L5 M5 S12"/>
											</layoutData>
										</Button>
										<Label text="Possui Estrutura de Armazenagem?" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit"
											visible="true"/>
										<SegmentedButton width="100%" class="sapUiSmallMarginBottom" selectedKey="{yearlyVisitModel>/HCP_STORAGE_STRUCTURE}"
											selectionChange="_onInputStorageStructure">
											<items>
												<SegmentedButtonItem text="Sim" key="1"/>
												<SegmentedButtonItem text="Não" key="0"/>
											</items>
										</SegmentedButton>
										<Label text="Tipo de Armazenagem" design="Standard" width="100%" required="{yearlyVisitModel>/yesStorage}" textAlign="Begin"
											textDirection="Inherit" visible="{yearlyVisitModel>/yesStorage}"/>
										<MultiComboBox
											items="{ path: '/Storage_Type', length: 999999 , filters: [{path: 'HCP_TYPE', operator: 'EQ', value1: 'Yearly'}, {path: 'HCP_ACTIVE', operator: 'EQ', value1: '1'}], sorter: { path: 'HCP_ID',descending: false}}"
											visible="{yearlyVisitModel>/yesStorage}" placeholder="Selecione o Tipo de Armazenagem" required="{yearlyVisitModel>/yesStorage}"
											editable="true" selectedKeys="{yearlyVisitModel>/negotiationInputs}" enabled="true" valueState="None" selectionChange="_validateForm"
											change="_validateForm">
											<items>
												<core:Item text="{HCP_DESC}" enabled="true" key="{HCP_ID}"/>
											</items>
										</MultiComboBox>
										<Label text="{i18n>textWhatsCapacity}" design="Standard" width="100%" required="{yearlyVisitModel>/yesStorage}" textAlign="Begin"
											textDirection="Inherit" visible="true"/>
										<Input placeholder="{i18n>textWhatsCapacityField}" name="HCP_CAPACITY"
											value="{ path: 'yearlyVisitModel>/HCP_CAPACITY' , type: 'sap.ui.model.type.Float', formatOptions: { groupingEnabled: true, groupingSeparator: '.', decimalSeparator: ',', maxFractionDigits: 2 }}"
											type="Tel" showValueHelp="false" enabled="true" required="{yearlyVisitModel>/yesStorage}" visible="{yearlyVisitModel>/yesStorage}"
											width="100%" valueHelpOnly="false" maxLength="0" liveChange="_valideInputNumber" change="_valideInputNumber"/>
										<Label text="{i18n>textWhatsSilosBag}" design="Standard" width="100%" required="{yearlyVisitModel>/yesStorage}" textAlign="Begin"
											textDirection="Inherit" visible="{yearlyVisitModel>/yesStorage}"/>
										<SegmentedButton width="100%" class="sapUiSmallMarginBottom" selectedKey="{yearlyVisitModel>/HCP_SILOS_BAG}"
											selectionChange="_onInputSilosStructure" visible="{yearlyVisitModel>/yesStorage}">
											<items>
												<SegmentedButtonItem text="{i18n>textYes}" key="1"/>
												<SegmentedButtonItem text="{i18n>textNo}" key="0"/>
											</items>
										</SegmentedButton>
										<Label text="{i18n>textWhatsSilosVolume}" design="Standard" width="100%" required="{yearlyVisitModel>/yesSilosBag}" textAlign="Begin"
											textDirection="Inherit" visible="{yearlyVisitModel>/yesSilosBag}"/>
										<Input placeholder="{i18n>textWhatsSilosVolumeField}" name="HCP_SILO_VOLUME"
											value="{ path: 'yearlyVisitModel>/HCP_SILO_VOLUME' , type: 'sap.ui.model.type.Float', formatOptions: { groupingEnabled: true, groupingSeparator: '.', decimalSeparator: ',', maxFractionDigits: 2 }}"
											type="Tel" showValueHelp="false" enabled="true" required="{yearlyVisitModel>/yesSilosBag}" visible="{yearlyVisitModel>/yesSilosBag}"
											width="100%" valueHelpOnly="false" maxLength="0" liveChange="_valideInputNumber" change="_valideInputNumber"/>
										<Label text="Realiza Barter/Troca?" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<SegmentedButton width="100%" class="sapUiSmallMarginBottom" selectedKey="{yearlyVisitModel>/HCP_BARTER_EXCHANGE}"
											selectionChange="_onInputBarterExchange">
											<items>
												<SegmentedButtonItem text="Sim" key="1"/>
												<SegmentedButtonItem text="Não" key="0"/>
											</items>
										</SegmentedButton>
										<Label text="Quantidade % " design="Standard" width="100%" required="{yearlyVisitModel>/yesBarter}" textAlign="Begin"
											textDirection="Inherit" visible="{yearlyVisitModel>/yesBarter}"/>
										<custom:CustomSlider min="0" max="100" value="{yearlyVisitModel>/HCP_AMOUNT}" step="1" enabled="true"
											visible="{yearlyVisitModel>/yesBarter}" width="100%" progress="true" change="_validateForm" showAdvancedTooltip="false">
											<custom:layoutData>
												<l:GridData span="L4 M4 S9"/>
											</custom:layoutData>
										</custom:CustomSlider>
										<NumericContent animateTextChange="true" value="{yearlyVisitModel>/HCP_AMOUNT}" width="" scale="%" class="percentageIndicator"
											withMargin="true" visible="{yearlyVisitModel>/yesBarter}">
											<layoutData>
												<l:GridData span="L1 M1 S3"/>
											</layoutData>
										</NumericContent>
										<Label text="Certificações?" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<MultiComboBox
											items="{ path: '/Visit_Certifications', length: 999999 , filters: [{path: 'HCP_LAND1', operator: 'EQ', value1: 'BR'}, {path: 'HCP_ACTIVE', operator: 'EQ', value1: '1'} ], sorter: { path: 'HCP_ID',descending: false}}"
											placeholder="Selecione as Certificações" required="false" editable="true" selectedKeys="{yearlyVisitModel>/certifications}" enabled="true"
											valueState="None" selectionChange="_validateInputCertification">
											<items>
												<core:Item text="{HCP_DESC}" enabled="true" key="{HCP_ID}"/>
											</items>
										</MultiComboBox>
										<Label text="{i18n>textOthers}" design="Standard" width="100%" required="{yearlyVisitModel>/yesOthers}" textAlign="Begin"
											textDirection="Inherit" visible="true"/>
										<TextArea rows="2" placeholder="Descritivo para outros" visible="{yearlyVisitModel>/yesOthers}" value="{yearlyVisitModel>/HCP_OTHERS}"
											cols="20" maxLength="255" wrapping="None" enabled="true" liveChange="_validateForm" required="{yearlyVisitModel>/yesOthers}"/>
										<Label text="Indique o Principal Parceiro de Insumos" design="Standard" width="100%" required="true" textAlign="Begin"
											textDirection="Inherit" visible="true"/>
										<SegmentedButton selectedKey="{yearlyVisitModel>/partnerProspect}" width="100%" class="sapUiSmallMarginBottom"
											selectionChange="_onInputFormSelect">
											<items>
												<SegmentedButtonItem text="Fornecedor" key="Fornecedor"/>
												<SegmentedButtonItem text="Prospect" key="Prospect"/>
											</items>
										</SegmentedButton>
										<Label text="Base de Fornecedores BRF?" design="Standard" width="100%" required="{yearlyVisitModel>/yesPartnerBRF}" textAlign="Begin"
											textDirection="Inherit" visible="{yearlyVisitModel>/yesPartnerBRF}"/>
										<SegmentedButton width="100%" class="sapUiSmallMarginBottom" selectedKey="{yearlyVisitModel>/HCP_PARTNER_BRF_SUP}"
											selectionChange="_onInputPartnerFormSelect" visible="{yearlyVisitModel>/yesPartnerBRF}">
											<items>
												<SegmentedButtonItem text="Sim" key="1"/>
												<SegmentedButtonItem text="Não" key="0"/>
											</items>
										</SegmentedButton>
										<Label text="Parceiro" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<Input id="yesPartner" visible="{yearlyVisitModel>/yesPartner}" required="{yearlyVisitModel>/yesPartner}" liveChange="_validateForm"
											change="_validateForm" editable="true" enabled="{true}" placeholder="Selecione o Parceiro" showValueHelp="true" type="Text"
											value="{yearlyVisitModel>/HCP_PARTNER}" description="{yearlyVisitModel>/partnerDesc}" valueHelpOnly="true"
											valueHelpRequest="_handlePartnerFilterPress"/>
										<Label text="Parceiro" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<Input id="noPartner" placeholder="Digite o Parceiro" type="Text" value="{yearlyVisitModel>/HCP_PARTNER}" showValueHelp="false"
											required="false" enabled="true" visible="{yearlyVisitModel>/noPartner}" width="100%" liveChange="_validateForm" change="_validateForm"
											valueHelpOnly="false" maxLength="0"/>
										<Label text="Prospect" design="Standard" width="100%" required="{yearlyVisitModel>/yesProspect}" textAlign="Begin" textDirection="Inherit"
											visible="{yearlyVisitModel>/yesProspect}"/>
										<custom:CustomComboBox id="yesProspect" placeholder="Selecione o Prospect" selectedKey="{yearlyVisitModel>/HCP_PARTNER}" editable="true"
											enabled="true" visible="{yearlyVisitModel>/yesProspect}" required="{yearlyVisitModel>/yesProspect}" width="auto" valueState="None"
											maxWidth="100%" selectionChange="_validateForm"
											items="{ path: '/Prospects', length: 999999, filters: [{path: 'HCP_STATUS', operator: 'EQ', value1: '1'}, {path: 'HCP_STATUS', operator: 'EQ', value1: '2'}] }">
											<custom:items>
												<core:Item text="{NAME1} {NAME2}" enabled="true" key="{HCP_PROSP_ID}"/>
											</custom:items>
										</custom:CustomComboBox>
									</form:content>
								</form:SimpleForm>
							</VBox>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
				<uxap:customAnchorBarButton/>
			</uxap:ObjectPageSection>
		</uxap:sections>
	</uxap:ObjectPageLayout>
</mvc:View>