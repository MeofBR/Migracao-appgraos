<mvc:View xmlns:mvc="sap.ui.core.mvc"
	controllerName="com.sap.build.standard.brfAppDeGraosModoEditar.controller.visitForm.NewPeriodicVisitForm" xmlns:uxap="sap.uxap"
	xmlns="sap.m" xmlns:form="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:semantic="sap.m.semantic"
	xmlns:u="sap.ui.unified" xmlns:custom="com.sap.build.standard.brfAppDeGraosModoEditar.controls">
	<uxap:ObjectPageLayout showTitleInHeaderContent="true" showHeaderContent="true" showAnchorBar="true" showFooter="true"
		subSectionLayout="TitleOnTop">
		<uxap:headerTitle>
			<uxap:ObjectPageDynamicHeaderTitle class="displayArrow">
				<uxap:breadcrumbs>
					<OverflowToolbarButton type="Transparent" icon="sap-icon://nav-back" tooltip="copy" press="onCancelPress"/>
				</uxap:breadcrumbs>
				<uxap:expandedHeading>
					<FlexBox wrap="Wrap" fitContainer="true" alignItems="Center">
						<Title text="Ficha de Visitas Periódico" wrapping="true" class="sapUiTinyMarginEnd sapUiTinyMarginTop"/>
					</FlexBox>
				</uxap:expandedHeading>
				<uxap:expandedContent>
					<FlexBox wrap="Wrap" fitContainer="true" alignItems="Center">
						<Title
							text="{i18n>textVendorProspect}: {visitFormModel>/HCP_PROVIDER_ID} - {visitFormModel>/HCP_NAME_REGISTERED}"
							width="100%" textAlign="Begin" textDirection="Inherit" wrapping="true"/>
					</FlexBox>
				</uxap:expandedContent>
				<uxap:snappedContent>
					<FlexBox wrap="Wrap" fitContainer="true" alignItems="Center">
						<Title
							text="{i18n>textVendorProspect}: {visitFormModel>/HCP_PROVIDER_ID} - {visitFormModel>/HCP_NAME_REGISTERED}"
							width="100%" textAlign="Begin" textDirection="Inherit" wrapping="true"/>
					</FlexBox>
				</uxap:snappedContent>
				<uxap:navigationActions>
					<Button text="Salvar" type="Emphasized" iconFirst="true" width="auto" enabled="{visitFormModel>/enableSave}" visible="true"
						iconDensityAware="false" press="onSavePress"/>
				</uxap:navigationActions>
			</uxap:ObjectPageDynamicHeaderTitle>
		</uxap:headerTitle>
		<uxap:sections>
			<uxap:ObjectPageSection showTitle="true" title="Cadastro" titleUppercase="true" visible="true">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="Cadastro" mode="Expanded">
						<uxap:blocks>
							<VBox>
								<form:SimpleForm class="simpleForm" columnsL="2" columnsM="2" columnsXL="1" editable="true" emptySpanL="4" emptySpanM="4" emptySpanXL="4"
									id="newEntitySimpleForm" labelSpanL="3" labelSpanM="3" labelSpanXL="3" layout="ResponsiveGridLayout" maxContainerCols="2" minWidth="2048"
									title="{= ${viewModel>/mode} === 'edit'? ${i18n>editViewTitle2} : ${i18n>createViewTitle2}}">
									<form:content>
										<Label text="Quais são as Ferramentas de Comercialização Utilizadas?" design="Standard" width="100%" required="true" textAlign="Begin"
											textDirection="Inherit" visible="true"/>
										<MultiComboBox
											items="{ path: '/Visit_Tools', length: 999999 , filters: {path: 'HCP_LAND1', operator: 'EQ', value1: 'BR'}, sorter: { path: 'HCP_ID',descending: false}}"
											placeholder="Selecione as Ferramentas de Comercialização Utilizadas" required="true" editable="true"
											selectedKeys="{visitFormModel>/toolsInputs}" enabled="true" valueState="None" selectionChange="_validateToolsInputs">
											<items>
												<core:Item text="{HCP_DESC}" enabled="true" key="{HCP_ID}"/>
											</items>
										</MultiComboBox>
										<Label text="Outras" design="Standard" width="100%" required="{visitFormModel>/yesOthersTools}" textAlign="Begin" textDirection="Inherit"
											visible="true"/>
										<TextArea rows="2" value="{visitFormModel>/HCP_OTHERS_TOOLS}" liveChange="_validateForm" placeholder="Descritivo de Outras Ferramentas"
											cols="20" maxLength="255" wrapping="None" enabled="true" required="{visitFormModel>/yesOthersTools}"
											visible="{visitFormModel>/yesOthersTools}"/>
										<Label text="Descreva as modalidades de contrato mais utilizadas" design="Standard" width="100%" required="false" textAlign="Begin"
											textDirection="Inherit" visible="true"/>
										<TextArea placeholder="Ex: CBOT x 70 x BRL BMF - 5" rows="2" value="{visitFormModel>/HCP_CONTRACT_MODALITY}" liveChange="_validateForm"
											cols="20" maxLength="255" wrapping="None" visible="true" enabled="true" required="false"/>
										<Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<form:SimpleForm class="simpleForm" width="100%" columnsL="1" columnsM="1" columnsXL="1" editable="true" emptySpanL="4" emptySpanM="0"
											emptySpanXL="0" id="newCultureSimpleForm" labelSpanL="3" labelSpanXL="3" labelSpanM="3" layout="ResponsiveGridLayout" maxContainerCols="2"
											minWidth="2048">
											<form:content></form:content>
										</form:SimpleForm>
										<Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<Button class="sapUiLargeMarginBottom" id="cultureFormID" text="Adicionar Nova Cultura" type="Emphasized" iconFirst="true" enabled="true"
											visible="true" iconDensityAware="false" press="_onAddCultureType">
											<customData>
												<core:CustomData key="id" value="newCultureSimpleForm"/>
											</customData>
											<layoutData>
												<l:GridData span="L5 M5 S12"/>
											</layoutData>
										</Button>
										<!--<form:SimpleForm class="simpleForm" width="auto" columnsL="4" columnsM="4" editable="true" emptySpanL="4" emptySpanM="4" emptySpanS="7"-->
										<!--	id="newCultureSimpleForm" labelSpanL="3" labelSpanM="3" labelSpanS="12" layout="ResponsiveGridLayout" maxContainerCols="2" minWidth="1024"-->
										<!--	title="">-->
										<!--	<form:content>-->
										<!--		<Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>-->
										<!--		<Button id="cultureFormID" text="Adicionar Nova Cultura" type="Emphasized" iconFirst="true" enabled="true" visible="true"-->
										<!--			iconDensityAware="false" press="_onAddCultureType"></Button>-->
										<!--	</form:content>-->
										<!--	<form:customData>-->
										<!--		<core:CustomData key="id" value="newCultureSimpleForm"/>-->
										<!--	</form:customData>-->
										<!--</form:SimpleForm>-->
										<!--<Label text="Saldo/Volume Disponível para Venda em (T) " design="Standard" width="100%" required="false" textAlign="Begin"-->
										<!--	textDirection="Inherit" visible="true"/>-->
										<!--<Input placeholder="Ex: 100.00" type="Number" value="{visitFormModel>/HCP_AVAILABLE_VOLUME}" showValueHelp="false" enabled="true"-->
										<!--	visible="true" width="auto" valueHelpOnly="false" maxLength="100" change="_validateForm"/>-->
										<Label text="Qual o Local em que o Produto que não foi Comercializado está Armazenado?" design="Standard" required="true" textAlign="Begin"
											textDirection="Inherit" visible="true"/>
										<MultiComboBox
											items="{ path: '/Storage_Type', length: 999999 , filters: [{path: 'HCP_TYPE', operator: 'EQ', value1: 'Periodic'}, {path: 'HCP_ACTIVE', operator: 'EQ', value1: '1'}], sorter: { path: 'HCP_ID',descending: false}}"
											placeholder="Selecione Local" required="true" editable="true" selectedKeys="{visitFormModel>/storedProductInputs}" enabled="true"
											valueState="None" change="_validateForm" selectionChange="_validateForm">
											<items>
												<core:Item text="{HCP_DESC}" enabled="true" key="{HCP_ID}"/>
											</items>
										</MultiComboBox>
										<Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<TextArea placeholder="Descritivo" rows="2" value="{visitFormModel>/HCP_DESC_STORAGE_TYPE}" liveChange="_validateForm" cols="20"
											maxLength="255" wrapping="None" enabled="true" required="false" visible="true"/>
										<Label text="Certificações?" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<MultiComboBox
											items="{ path: '/Visit_Certifications', length: 999999 , filters: [{path: 'HCP_LAND1', operator: 'EQ', value1: 'BR'}, {path: 'HCP_ACTIVE', operator: 'EQ', value1: '1'} ], sorter: { path: 'HCP_ID',descending: false}}"
											placeholder="Selecione as Certificações" required="false" editable="true" selectedKeys="{visitFormModel>/certificationsInputs}"
											enabled="true" valueState="None" selectionChange="_validateInputCertification">
											<items>
												<core:Item text="{HCP_DESC}" enabled="true" key="{HCP_ID}"/>
											</items>
										</MultiComboBox>
										<Label text="Outros" design="Standard" width="100%" required="{visitFormModel>/yesOthersCertication}" textAlign="Begin"
											textDirection="Inherit" visible="true"/>
										<TextArea rows="2" value="{visitFormModel>/HCP_OTHERS}" liveChange="_validateForm" placeholder="Descritivo" cols="20" maxLength="255"
											wrapping="None" enabled="true" required="{visitFormModel>/yesOthersCertication}" visible="{visitFormModel>/yesOthersCertication}"/>
										<Label text="O que Falta para Aumentar o Volume de Comercialização com a BRF?" design="Standard" width="100%" required="true"
											textAlign="Begin" textDirection="Inherit" visible="true"/>
										<MultiComboBox
											items="{ path: '/Visit_Criterion', length: 999999 , filters: {path: 'HCP_LAND1', operator: 'EQ', value1: 'BR'}, sorter: { path: 'HCP_ID',descending: false}}"
											placeholder="Seleção do Critério" required="true" editable="true" selectedKeys="{visitFormModel>/criterionsInputs}" enabled="true"
											valueState="None" selectionChange="_validateCriterionInputs">
											<items>
												<core:Item text="{HCP_DESC}" enabled="true" key="{HCP_ID}"/>
											</items>
										</MultiComboBox>
										<Label text="Outros" design="Standard" width="100%" required="{visitFormModel>/yesOthersCriterion}" textAlign="Begin"
											textDirection="Inherit" visible="true"/>
										<TextArea rows="2" value="{visitFormModel>/HCP_OTHERS_CRITERION}" liveChange="_validateForm" placeholder="Descritivo" cols="20"
											maxLength="255" wrapping="None" enabled="true" required="{visitFormModel>/yesOthersCriterion}"
											visible="{visitFormModel>/yesOthersCriterion}"/>
										<!--<custom:CustomComboBox-->
										<!--	items="{ path: '/Visit_Criterion', length: 999999 , filters: {path: 'HCP_LAND1', operator: 'EQ', value1: 'BR'}, sorter: { path: 'HCP_ID',descending: false}}"-->
										<!--	placeholder="Seleção do Critério" editable="true" selectedKey="{visitFormModel>/HCP_CRITERION}" enabled="true" visible="true" width="auto"-->
										<!--	valueState="None" maxWidth="100%" selectionChange="_validateInputCriterion">-->
										<!--	<custom:items>-->
										<!--		<core:Item text="{HCP_DESC}" enabled="true" key="{HCP_ID}"/>-->
										<!--	</custom:items>-->
										<!--</custom:CustomComboBox>-->
										<Label text="Qual o Fator mais Relevante para a Tomada de Decisão de Comercialização?" design="Standard" width="100%" required="false"
											textAlign="Begin" textDirection="Inherit" visible="true"/>
										<TextArea rows="2" value="{visitFormModel>/HCP_DECISION_FACTOR}" liveChange="_validateForm" placeholder="Descritivo" cols="20"
											maxLength="255" wrapping="None" enabled="true" visible="true"/>
										<Label text="Relato da Visita?" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<TextArea placeholder="Descritivo" rows="3" value="{visitFormModel>/HCP_REPORT_VISIT}" liveChange="_validateForm" cols="20" maxLength="10000"
											wrapping="None" visible="true" enabled="true" required="true"/>
										<!--<Input placeholder="Descritivo" type="Text" value="{visitFormModel>/HCP_REPORT_VISIT}" showValueHelp="false" enabled="true" visible="true"-->
										<!--	width="auto" valueHelpOnly="false" maxLength="100" change="_validateForm"/>-->
										<Label text="Há necessidade de revisar algum item da ficha anual?" design="Standard" width="100%" required="false" textAlign="Begin"
											textDirection="Inherit" visible="true"/>
										<SegmentedButton width="100%" class="sapUiSmallMarginBottom" selectedKey="{visitFormModel>/HCP_CHECK_MANUAL_TAB}"
											selectionChange="_validateForm">
											<items>
												<SegmentedButtonItem text="Sim" key="1"/>
												<SegmentedButtonItem text="Não" key="0"/>
											</items>
										</SegmentedButton>
										<!--<Label text="Deseja agendar a próxima visita?" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"-->
										<!--	visible="true"/>-->
										<!--<SegmentedButton width="auto" class="sapUiSmallMarginBottom" selectedKey="{visitFormModel>/HCP_SCHEDULE_NEXT_VISIT}"-->
										<!--	selectionChange="_validateForm">-->
										<!--	<items>-->
										<!--		<SegmentedButtonItem text="Sim" key="1"/>-->
										<!--		<SegmentedButtonItem text="Não" key="0"/>-->
										<!--	</items>-->
										<!--</SegmentedButton>-->
									</form:content>
								</form:SimpleForm>
							</VBox>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
				<uxap:customAnchorBarButton/>
			</uxap:ObjectPageSection>
		</uxap:sections>
	</uxap:ObjectPageLayout>
</mvc:View>