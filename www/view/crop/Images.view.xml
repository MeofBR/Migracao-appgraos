<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.brfAppDeGraosModoEditar.controller.crop.Images" xmlns="sap.m"
	xmlns:f="sap.ui.layout.form" xmlns:grid="sap.ui.layout.cssgrid" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout"
	xmlns:custom="com.sap.build.standard.brfAppDeGraosModoEditar.controls" xmlns:z="sap.f" xmlns:u="sap.ui.unified">
	<Page title="Selecione" showNavButton="true" navButtonPress="navBack" showFooter="true">
		<content>
			<Carousel class="box-img-grande sapUiContentPadding gridResponsive" loop="true" id="carouselID">
				<pages>
					<Image id="imgCarousel1" src="" densityAware="false"/>
					<Image id="imgCarousel2" src="" densityAware="false"/>
					<Image id="imgCarousel3" src="" densityAware="false"/>
					<Image id="imgCarousel4" src="" densityAware="false"/>
				</pages>
			</Carousel>
			<l:VerticalLayout width="100%" class="gridWrapper heightImagens">
				<l:Grid containerQuery="true" class="shadowBox collumPicture" defaultSpan="XL2 L2 M2 S3">
					<VBox class="demoBox box-img-menor">
						<VBox class="heightImageBox">
							<Image id="img1" src="sap-icon://camera" class="heightFixed" densityAware="false" press="onImagePress"/>
						</VBox>
						<VBox alignItems="Center">
							<Button icon="sap-icon://delete" class="buttonDelete button_red" press="onImageDelete"
								enabled="{= ${imagesModel>/enableEditConsult} &amp;&amp; ${imagesModel>/slot1Enabled}}" visible="{= ${imagesModel>/enableEditConsult} &amp;&amp; ${imagesModel>/slot1Enabled}}"/>
							<!--<Title level="H1" text="{imagesModel>/textImg1}" visible="{imagesModel>/enableTextImg1}"/>-->
							<ObjectStatus text="{imagesModel>/textImg1}" state="{= ${imagesModel>/textImg1} === 'Online' ? 'Success' : 'Information'}"
								visible="{imagesModel>/enableTextImg1}"/>
						</VBox>
						<!--<Title level="H1" text="Slot Ocupado"/>-->
					</VBox>
					<VBox class="demoBox box-img-menor">
						<VBox class="heightImageBox">
							<Image id="img2" src="sap-icon://camera" class="heightFixed" densityAware="false" press="onImagePress"/>
						</VBox>
						<VBox alignItems="Center">
							<Button icon="sap-icon://delete" class="button_red buttonDelete" press="onImageDelete"
								enabled="{= ${imagesModel>/enableEditConsult} &amp;&amp; ${imagesModel>/slot2Enabled}}" visible="{= ${imagesModel>/enableEditConsult} &amp;&amp; ${imagesModel>/slot1Enabled}}"/>
							<!--<Title level="H1" text="{imagesModel>/textImg2}" visible="{imagesModel>/enableTextImg2}"/>-->
							<ObjectStatus text="{imagesModel>/textImg2}" state="{= ${imagesModel>/textImg2} === 'Online' ? 'Success' : 'Information'}"
								visible="{imagesModel>/enableTextImg2}"/>
						</VBox>
					</VBox>
					<VBox class="demoBox box-img-menor">
						<VBox class="heightImageBox">
							<Image id="img3" src="sap-icon://camera" class="heightFixed" densityAware="false" press="onImagePress"/>
						</VBox>
						<VBox alignItems="Center">
							<Button icon="sap-icon://delete" class="buttonDelete" press="onImageDelete"
								enabled="{= ${imagesModel>/enableEditConsult} &amp;&amp; ${imagesModel>/slot3Enabled}}" visible="{= ${imagesModel>/enableEditConsult} &amp;&amp; ${imagesModel>/slot1Enabled}}"/>
							<!--<Title level="H1" text="{imagesModel>/textImg3}" visible="{imagesModel>/enableTextImg3}"/>-->
							<ObjectStatus text="{imagesModel>/textImg3}" state="{= ${imagesModel>/textImg3} === 'Online' ? 'Success' : 'Information'}"
								visible="{imagesModel>/enableTextImg3}"/>
						</VBox>
					</VBox>
					<VBox class="demoBox box-img-menor">
						<VBox class="heightImageBox" alignItems="Center">
							<Image id="img4" src="sap-icon://camera" class="heightFixed" densityAware="false" press="onImagePress"/>
						</VBox>
						<VBox alignItems="Center">
							<Button icon="sap-icon://delete" class="buttonDelete" press="onImageDelete"
								enabled="{= ${imagesModel>/enableEditConsult} &amp;&amp; ${imagesModel>/slot4Enabled}}" visible="{= ${imagesModel>/enableEditConsult} &amp;&amp; ${imagesModel>/slot1Enabled}}"/>
							<!--<Title level="H1" text="{imagesModel>/textImg4}" visible="{imagesModel>/enableTextImg4}"/>-->
							<ObjectStatus text="{imagesModel>/textImg4}" state="{= ${imagesModel>/textImg4} === 'Online' ? 'Success' : 'Information'}"
								visible="{imagesModel>/enableTextImg4}"/>
						</VBox>
					</VBox>
					<VBox class="demoBox" alignItems="Center">
						<u:FileUploader buttonText="Adicionar Foto" id="fileUploaderID" icon="sap-icon://add-photo" buttonOnly="true"
							uploadComplete="handleUploadComplete" change="handleFileUploaderChange" style="Emphasized" sendXHR="true"
							enabled="{imagesModel>/enableEditConsult}" visible="{imagesModel>/enableEditConsult}"/>
					</VBox>
				</l:Grid>
			</l:VerticalLayout>
		</content>
		<footer>
			<OverflowToolbar width="100%" height="auto" design="Auto" enabled="true" visible="true">
				<content>
					<ToolbarSpacer/>
					<Button text="Voltar" type="Emphasized" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="navBack"/>
				</content>
			</OverflowToolbar>
		</footer>
	</Page>
</mvc:View>