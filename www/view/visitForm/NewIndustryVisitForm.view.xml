<mvc:View xmlns:mvc="sap.ui.core.mvc"
	controllerName="com.sap.build.standard.brfAppDeGraosModoEditar.controller.visitForm.NewIndustryVisitForm" xmlns:uxap="sap.uxap"
	xmlns="sap.m" xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns:semantic="sap.m.semantic" xmlns:u="sap.ui.unified"
	xmlns:l="sap.ui.layout" xmlns:custom="com.sap.build.standard.brfAppDeGraosModoEditar.controls">
	<uxap:ObjectPageLayout showTitleInHeaderContent="true" showHeaderContent="true" showAnchorBar="true" showFooter="true"
		subSectionLayout="TitleOnTop">
		<uxap:headerTitle>
			<uxap:ObjectPageDynamicHeaderTitle class="displayArrow">
				<uxap:breadcrumbs>
					<OverflowToolbarButton type="Transparent" icon="sap-icon://nav-back" tooltip="copy" press="onCancelPress"/>
				</uxap:breadcrumbs>
				<uxap:expandedHeading>
					<FlexBox wrap="Wrap" fitContainer="true" alignItems="Center">
						<Title text="Ficha de Visitas Indústria" wrapping="true" class="sapUiTinyMarginEnd sapUiTinyMarginTop"/>
					</FlexBox>
				</uxap:expandedHeading>
				<uxap:expandedContent>
					<FlexBox wrap="Wrap" fitContainer="true" alignItems="Center">
						<Title
							text="{i18n>textVendorProspect}: {industryVisitModel>/HCP_PROVIDER_ID} - {industryVisitModel>/HCP_NAME_REGISTERED}"
							width="100%" textAlign="Begin" textDirection="Inherit" wrapping="true"/>
					</FlexBox>
				</uxap:expandedContent>
				<uxap:snappedContent>
					<FlexBox wrap="Wrap" fitContainer="true" alignItems="Center">
						<Title
							text="{i18n>textVendorProspect}: {industryVisitModel>/HCP_PROVIDER_ID} - {industryVisitModel>/HCP_NAME_REGISTERED}"
							width="100%" textAlign="Begin" textDirection="Inherit" wrapping="true"/>
					</FlexBox>
				</uxap:snappedContent>
				<uxap:navigationActions>
					<Button text="Salvar" type="Emphasized" iconFirst="true" width="auto"
						enabled="{= ${industryVisitModel>/enableSave} &amp;&amp; ${industryVisitModel>/enableCreateRegioValid}}" visible="true"
						iconDensityAware="false" press="onSavePress"/>
				</uxap:navigationActions>
			</uxap:ObjectPageDynamicHeaderTitle>
		</uxap:headerTitle>
		<uxap:sections>
			<uxap:ObjectPageSection showTitle="true" title="Cadastro" titleUppercase="true" visible="true">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="Cadastro" mode="Expanded">
						<uxap:blocks>
							<VBox>
								<form:SimpleForm class="simpleForm" columnsL="1" columnsM="1" columnsXL="1" editable="true" emptySpanL="4" emptySpanM="4" emptySpanXL="4"
									id="newEntitySimpleForm" labelSpanL="3" labelSpanM="3" labelSpanXL="3" layout="ResponsiveGridLayout" maxContainerCols="2" minWidth="2048">
									<form:content>
										<Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"
											visible="{= ${industryVisitModel>/enableCreateRegioValid} === true ? false : true}"/>
										<MessageStrip text="Estado não permitido para o seu perfil" type="Error" showIcon="true" showCloseButton="false"
											visible="{= ${industryVisitModel>/enableCreateRegioValid} === true ? false : true}" class="sapUiMediumMarginTop"/>
										<Label text="UF" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<custom:CustomComboBox placeholder="Selecione o Estado" selectedKey="{industryVisitModel>/HCP_UF}" editable="true" enabled="true"
											visible="true" required="true" width="100%" valueState="None" maxWidth="100%" change="_onInputUfFormSelect"
											selectionChange="_onInputUfFormSelect"
											items="{ path: '/View_States', length: 999999 , filters: {path: 'LAND1', operator: 'EQ', value1: 'BR'}, sorter: { path: 'BLAND',descending: false}}">
											<custom:items>
												<core:Item text="{BLAND} - {BEZEI}" enabled="true" key="{BLAND}"/>
											</custom:items>
										</custom:CustomComboBox>
										<Label id="countyLabel" text="Município" design="Standard" width="100%" required="{industryVisitModel>/yesCity}" textAlign="Begin"
											textDirection="Inherit" visible="true"/>
										<custom:CustomComboBox id="county" items="{ path: '/View_City', length: 999999, sorter: { path: 'TAXJURCODE',descending: false} }"
											placeholder="Selecione o Município" editable="true" enabled="{industryVisitModel>/enableCounty}" required="{industryVisitModel>/yesCity}"
											visible="true" width="auto" valueState="None" maxWidth="100%" selectedKey="{industryVisitModel>/HCP_CITY}" selectionChange="_validateForm">
											<custom:items>
												<core:Item text="{TEXT}" enabled="true" key="{TAXJURCODE}"/>
											</custom:items>
										</custom:CustomComboBox>
										<Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<form:SimpleForm class="simpleForm" width="100%" columnsL="1" columnsM="1" columnsXL="1" editable="true" emptySpanL="4" emptySpanM="0"
											emptySpanXL="0" id="newMaterialSimpleForm" labelSpanL="3" labelSpanM="3" labelSpanXL="3" layout="ResponsiveGridLayout" maxContainerCols="2"
											minWidth="2048">
											<form:content></form:content>
										</form:SimpleForm>
										<Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<Button text="Adicionar Novo Material" type="Emphasized" iconFirst="true" enabled="true" visible="true" iconDensityAware="false"
											press="_onAddNewForm">
											<customData>
												<core:CustomData key="id" value="newMaterialSimpleForm"/>
											</customData>
											<layoutData>
												<l:GridData span="L5 M5 S12"/>
											</layoutData>
										</Button>
										<Label text="Quais são os tipos de negociação feitas pelo fornecedor?" design="Standard" width="100%" required="true" textAlign="Begin"
											textDirection="Inherit" visible="true"/>
										<MultiComboBox
											items="{ path: '/Visit_Tools', length: 999999 , filters: {path: 'HCP_LAND1', operator: 'EQ', value1: 'BR'}, sorter: { path: 'HCP_ID',descending: false}}"
											placeholder="Selecione os Tipos de Negociação" required="true" editable="true" selectedKeys="{industryVisitModel>/toolsInputs}"
											enabled="true" valueState="None" selectionChange="_validateToolsInputs">
											<items>
												<core:Item text="{HCP_DESC}" enabled="true" key="{HCP_ID}"/>
											</items>
										</MultiComboBox>
										<Label text="Outras" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<TextArea placeholder="Descritivo de Outras Negociações" rows="2" value="{industryVisitModel>/HCP_OTHERS_TOOLS}" liveChange="_validateForm"
											cols="20" maxLength="255" wrapping="None" enabled="true" required="false" visible="{industryVisitModel>/yesOthersTools}"/>
										<Label text="Descreva as modalidades de contrato mais utilizada" design="Standard" width="100%" required="false" textAlign="Begin"
											textDirection="Inherit" visible="true"/>
										<TextArea rows="2" placeholder="Ex: (CBOT x 70% x BRL; BM&amp;F - 5)" visible="true" value="{industryVisitModel>/HCP_CONTRACT_MODALITY}"
											cols="20" maxLength="255" wrapping="None" required="false" enabled="true" liveChange="_validateForm"/>
										<Label text="Qual o Principal Mês de negociação antecipada?" design="Standard" width="100%" required="true" textAlign="Begin"
											textDirection="Inherit" visible="true"/>
										<MultiComboBox placeholder="Selecione o Mês" required="true" editable="true" selectedKeys="{industryVisitModel>/negotiationFramesInputs}"
											enabled="true" valueState="None" selectionChange="_validateForm" change="_validateForm">
											<items>
												<core:Item text="Janeiro" enabled="true" key="HCP_JANUARY"/>
												<core:Item text="Fevereiro" enabled="true" key="HCP_FEBRUARY"/>
												<core:Item text="Março" enabled="true" key="HCP_MARCH"/>
												<core:Item text="Abril" enabled="true" key="HCP_APRIL"/>
												<core:Item text="Maio" enabled="true" key="HCP_MAY"/>
												<core:Item text="Junho" enabled="true" key="HCP_JUNE"/>
												<core:Item text="Julho" enabled="true" key="HCP_JULY"/>
												<core:Item text="Agosto" enabled="true" key="HCP_AUGUST"/>
												<core:Item text="Setembro" enabled="true" key="HCP_SEPTEMBER"/>
												<core:Item text="Outubro" enabled="true" key="HCP_OCTOBER"/>
												<core:Item text="Novembro" enabled="true" key="HCP_NOVEMBER"/>
												<core:Item text="Dezembro" enabled="true" key="HCP_DECEMBER"/>
											</items>
										</MultiComboBox>
										<Label text="Possui programa de Barter com seus fornecedores/produtores?" design="Standard" width="100%" required="true" textAlign="Begin"
											textDirection="Inherit" visible="true"/>
										<SegmentedButton width="100%" class="sapUiSmallMarginBottom" selectedKey="{industryVisitModel>/HCP_PARTNER_PRODUCERS}"
											selectionChange="_onbarterExchangeFormSelect">
											<items>
												<SegmentedButtonItem text="Sim" key="1"/>
												<SegmentedButtonItem text="Não" key="0"/>
											</items>
										</SegmentedButton>
										<Label text="Descrição do Programa Barter" design="Standard" width="100%" required="{industryVisitModel>/yesBartner}" textAlign="Begin"
											textDirection="Inherit" visible="{industryVisitModel>/yesBartner}"/>
										<TextArea rows="2" placeholder="Observação sobre o programa de Barter" visible="{industryVisitModel>/yesBartner}"
											value="{industryVisitModel>/HCP_OTHER_BARTER}" cols="20" maxLength="255" wrapping="None" required="{industryVisitModel>/yesBartner}"
											enabled="true" liveChange="_validateForm"/>
										<Label text="Possui Frota Própria para Transporte de Material?" design="Standard" width="100%" required="false" textAlign="Begin"
											textDirection="Inherit" visible="true"/>
										<SegmentedButton width="100%" class="sapUiSmallMarginBottom" selectedKey="{industryVisitModel>/HCP_OWN_FLEET}"
											selectionChange="_onOwnFleetFormSelect">
											<items>
												<SegmentedButtonItem text="Sim" key="1"/>
												<SegmentedButtonItem text="Não" key="0"/>
											</items>
										</SegmentedButton>
										<Label text="Qual o % de transporte com a frota própria?" design="Standard" width="100%" required="{industryVisitModel>/yesOwnFleet}"
											textAlign="Begin" textDirection="Inherit" visible="{industryVisitModel>/yesOwnFleet}"/>
										<custom:CustomSlider min="0" max="100" value="{industryVisitModel>/HCP_PERCENT_OWN_FLEET}" step="1" enabled="true"
											visible="{industryVisitModel>/yesOwnFleet}" width="auto" progress="true" liveChange="_validateForm" showAdvancedTooltip="false">
											<custom:layoutData>
												<l:GridData span="L4 M4 S9"/>
											</custom:layoutData>
										</custom:CustomSlider>
										<NumericContent animateTextChange="true" value="{industryVisitModel>/HCP_PERCENT_OWN_FLEET}" width="" scale="%" class="percentageIndicator"
											withMargin="true" visible="{industryVisitModel>/yesOwnFleet}">
											<layoutData>
												<l:GridData span="L1 M1 S3"/>
											</layoutData>
										</NumericContent>
										<Label text="O que Falta para Aumentar o Volume de Comercialização com a BRF?" design="Standard" width="100%" required="true"
											textAlign="Begin" textDirection="Inherit" visible="true"/>
										<!--<custom:CustomComboBox-->
										<!--	items="{ path: '/Visit_Criterion', length: 999999 , filters: {path: 'HCP_LAND1', operator: 'EQ', value1: 'BR'}, sorter: { path: 'HCP_ID',descending: false}}"-->
										<!--	placeholder="Seleção do Critério" editable="true" selectedKey="{industryVisitModel>/HCP_CRITERION}" enabled="true" visible="true"-->
										<!--	width="auto" valueState="None" maxWidth="100%" selectionChange="_validateInputCriterion">-->
										<!--	<custom:items>-->
										<!--		<core:Item text="{HCP_DESC}" enabled="true" key="{HCP_ID}"/>-->
										<!--	</custom:items>-->
										<!--</custom:CustomComboBox>-->
										<MultiComboBox
											items="{ path: '/Visit_Criterion', length: 999999 , filters: {path: 'HCP_LAND1', operator: 'EQ', value1: 'BR'}, sorter: { path: 'HCP_ID',descending: false}}"
											placeholder="Seleção do Critério" required="true" editable="true" selectedKeys="{industryVisitModel>/criterionsInputs}" enabled="true"
											valueState="None" selectionChange="_validateCriterionInputs">
											<items>
												<core:Item text="{HCP_DESC}" enabled="true" key="{HCP_ID}"/>
											</items>
										</MultiComboBox>
										<Label text="" design="Standard" width="100%" required="{industryVisitModel>/yesOthersCriterion}" textAlign="Begin" textDirection="Inherit"
											visible="true"/>
										<TextArea rows="2" placeholder="Descritivo em caso de outros" visible="{industryVisitModel>/yesOthersCriterion}"
											required="{industryVisitModel>/yesOthersCriterion}" value="{industryVisitModel>/HCP_OTHER_CRITERION}" cols="20" maxLength="255"
											wrapping="None" enabled="true" liveChange="_validateForm"/>
										<Label text="Qual o Fator mais Relevante para a Tomada de Decisão de Comercialização Anual?" design="Standard" width="auto" required="true"
											textAlign="Begin" textDirection="Inherit" visible="true"/>
										<TextArea rows="2" placeholder="Descritivo do Fator" visible="true" value="{industryVisitModel>/HCP_DECISION_FACTOR}" cols="20"
											maxLength="255" wrapping="None" enabled="true" required="true" liveChange="_validateForm"/>
										<Label text="Certificações?" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<MultiComboBox
											items="{ path: '/Visit_Certifications', length: 999999 , filters: [{path: 'HCP_LAND1', operator: 'EQ', value1: 'BR'}, {path: 'HCP_ACTIVE', operator: 'EQ', value1: '1'} ], sorter: { path: 'HCP_ID',descending: false}}"
											placeholder="Selecione as Certificações" required="false" editable="true" selectedKeys="{industryVisitModel>/certificationsInputs}"
											enabled="true" valueState="None" selectionChange="_validateInputCertification">
											<items>
												<core:Item text="{HCP_DESC}" enabled="true" key="{HCP_ID}"/>
											</items>
										</MultiComboBox>
										<Label text="" design="Standard" width="100%" required="{industryVisitModel>/yesOthersCertication}" textAlign="Begin"
											textDirection="Inherit" visible="true"/>
										<TextArea rows="2" placeholder="Descritivo para outros" visible="{industryVisitModel>/yesOthersCertication}"
											value="{industryVisitModel>/HCP_OTHERS}" cols="20" maxLength="255" wrapping="None" enabled="true"
											required="{industryVisitModel>/yesOthersCertication}" liveChange="_validateForm"/>
										<!--<Label text="Indique o Principal Parceiro de Insumos: Base de Fornecedores BRF?" design="Standard" width="auto" required="true"-->
										<!--	textAlign="Begin" textDirection="Inherit" visible="true" class="sapUiSmallMarginBegin"/>-->
										<!--<SegmentedButton width="auto" class="sapUiSmallMarginBottom" selectedKey="{industryVisitModel>/HCP_PARTNER_BRF_SUP}"-->
										<!--	selectionChange="_onInputPartnerFormSelect">-->
										<!--	<items>-->
										<!--		<SegmentedButtonItem text="Sim" key="1"/>-->
										<!--		<SegmentedButtonItem text="Não" key="0"/>-->
										<!--	</items>-->
										<!--</SegmentedButton>-->
										<!--<Label text="Parceiro" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>-->
										<!--<Input id="yesPartner" visible="{industryVisitModel>/yesPartner}" required="{industryVisitModel>/yesPartner}" liveChange="_validateForm"-->
										<!--	change="_validateForm" editable="true" enabled="true" placeholder="Selecione o Parceiro" showValueHelp="true" type="Text"-->
										<!--	value="{industryVisitModel>/HCP_PARTNER}" description="{industryVisitModel>/partnerDesc}" valueHelpOnly="true"-->
										<!--	valueHelpRequest="_handlePartnerFilterPress"/>-->
										<!--<Label text="Parceiro" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>-->
										<!--<Input id="noPartner" placeholder="Digite o Parceiro" type="Text" value="{industryVisitModel>/HCP_PARTNER}" showValueHelp="false"-->
										<!--	enabled="true" visible="{industryVisitModel>/noPartner}" width="auto" required="false" liveChange="_validateForm" change="_validateForm"-->
										<!--	valueHelpOnly="false" maxLength="0"/>-->
										<!--<Label text="Deseja agendar a próxima visita?" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit"-->
										<!--	visible="true" class="sapUiSmallMarginBegin"/>-->
										<!--<SegmentedButton width="auto" class="sapUiSmallMarginBottom" selectedKey="{industryVisitModel>/HCP_SCHEDULE_NEXT_VISIT}"-->
										<!--	selectionChange="_validateForm">-->
										<!--	<items>-->
										<!--		<SegmentedButtonItem text="Sim" key="1"/>-->
										<!--		<SegmentedButtonItem text="Não" key="0"/>-->
										<!--	</items>-->
										<!--</SegmentedButton>-->
										<Label text="Relato da Visita?" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<TextArea rows="3" placeholder="Descritivo da Visitas" visible="true" value="{industryVisitModel>/HCP_REPORT_VISIT}" cols="20"
											maxLength="10000" wrapping="None" enabled="true" required="true" liveChange="_validateForm"/>
									</form:content>
								</form:SimpleForm>
							</VBox>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
				<uxap:customAnchorBarButton/>
			</uxap:ObjectPageSection>
		</uxap:sections>
	</uxap:ObjectPageLayout>
</mvc:View>