									<Label text="{i18n>textComModality}" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
									<custom:CustomComboBox placeholder="{i18n>textComModality}" selectedKey="{fixedOrderFormModel>/HCP_ZMODAL}" editable="true"
										enabled="{fixedOrderFormModel>/enabled}" visible="true" required="true" width="auto" valueState="None" maxWidth="100%"
										selectionChange="_validateForm"
										items="{ path: '/View_Domain', filters: {path: 'DOMNAME', operator: 'EQ', value1: 'ZMODAL'} , sorter: { path: 'VALPOS' } }">
										<custom:items>
											<core:Item text="{DDTEXT}" enabled="true" key="{DOMVALUE_L}"/>
										</custom:items>
									</custom:CustomComboBox>
									<!--Adiantamento-->
									<Label text="{i18n>textAdvance}" design="Standard" width="100%" textAlign="Begin" textDirection="Inherit" required="true" visible="true"/>
									<SegmentedButton selectedKey="{fixedOrderFormModel>/HCP_ZADIAMTO}" width="100%" selectionChange="_validateForm"
										enabled="{fixedOrderFormModel>/enabled}">
										<items>
											<SegmentedButtonItem text="{i18n>textYes}" key="S"/>
											<SegmentedButtonItem text="{i18n>textNo}" key="N"/>
										</items>
									</SegmentedButton>
									<!--Pedido Substituição-->
									<Label text="{i18n>textOrderReplacement}" design="Standard" width="100%" textAlign="Begin" textDirection="Inherit" required="true"
										visible="true"/>
									<SegmentedButton selectedKey="{fixedOrderFormModel>/HCP_SUBST}" width="100%" selectionChange="_onInputOrderReplacementFormSelect"
										enabled="{fixedOrderFormModel>/enabled}">
										<items>
											<SegmentedButtonItem text="{i18n>textYes}" key="S"/>
											<SegmentedButtonItem text="{i18n>textNo}" key="N"/>
										</items>
									</SegmentedButton>
									<!--Pedido-->
									<Label text="{i18n>textOriginalOrder}" design="Standard" width="100%" required="{fixedOrderFormModel>/yesOrdeRep}" textAlign="Begin"
										textDirection="Inherit" visible="{fixedOrderFormModel>/yesOrdeRep}"/>
									<Input placeholder="{i18n>placeEnterOriginalOrder}" name="HCP_EBELN_SUB" value="{fixedOrderFormModel>/HCP_EBELN_SUB}" type="Tel"
										showValueHelp="false" enabled="{fixedOrderFormModel>/enabled}" required="{fixedOrderFormModel>/yesOrdeRep}"
										visible="{fixedOrderFormModel>/yesOrdeRep}" width="100%" valueHelpOnly="false" maxLength="0" liveChange="_validateForm"
										change="_validateForm"/>
									<!--Motivo de Substituição-->
									<Label text="{i18n>textReasonReplacement}" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit"
										visible="{fixedOrderFormModel>/yesOrdeRep}"/>
									<custom:CustomComboBox placeholder="{i18n>placeSelectReasonReplacement}" selectedKey="{fixedOrderFormModel>/HCP_JUST_SUB}" editable="true"
										enabled="{fixedOrderFormModel>/enabled}" visible="{fixedOrderFormModel>/yesOrdeRep}" required="true" width="auto" valueState="None"
										maxWidth="100%" selectionChange="_validateForm"
										items="{ path: '/View_Domain', filters: {path: 'DOMNAME', operator: 'EQ', value1: 'ZZJUST_SUB'} , sorter: { path: 'VALPOS' } }">
										<custom:items>
											<core:Item text="{DDTEXT}" enabled="true" key="{DOMVALUE_L}"/>
										</custom:items>
									</custom:CustomComboBox>
									<!--Umidade-->
									<Label text="{i18n>textMoisture}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
									<Input placeholder="{i18n>placeEnterMoisture}" liveChange="_validateForm" change="_validateForm" name="HCP_ZUMIDADE"
										value="{ path: 'fixedOrderFormModel>/HCP_ZUMIDADE' , type: 'sap.ui.model.type.Float', formatOptions: { groupingEnabled: true, groupingSeparator: '.', decimalSeparator: ',', maxFractionDigits: 2 }}"
										type="Tel" showValueHelp="false" enabled="{fixedOrderFormModel>/enabled}" required="false" visible="true" width="100%" valueHelpOnly="false"
										maxLength="0"/>
									<!--Impureza-->
									<Label text="{i18n>textImpurity}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
									<Input placeholder="{i18n>placeEnterImpurity}" liveChange="_validateForm" change="_validateForm" name="HCP_ZIMPUREZA"
										value="{ path: 'fixedOrderFormModel>/HCP_ZIMPUREZA' , type: 'sap.ui.model.type.Float', formatOptions: { groupingEnabled: true, groupingSeparator: '.', decimalSeparator: ',', maxFractionDigits: 2 }}"
										type="Tel" showValueHelp="false" enabled="{fixedOrderFormModel>/enabled}" required="false" visible="true" width="100%" valueHelpOnly="false"
										maxLength="0"/>
									<!--Ardidos-->
									<Label text="{i18n>textBurned}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
									<Input placeholder="{i18n>placeEnterBurned}" liveChange="_validateForm" change="_validateForm" name="HCP_ZARDIDOS"
										value="{ path: 'fixedOrderFormModel>/HCP_ZARDIDOS' , type: 'sap.ui.model.type.Float', formatOptions: { groupingEnabled: true, groupingSeparator: '.', decimalSeparator: ',', maxFractionDigits: 2 }}"
										type="Tel" showValueHelp="false" enabled="{fixedOrderFormModel>/enabled}" required="false" visible="true" width="100%" valueHelpOnly="false"
										maxLength="0"/>
									<!--Contrato-->
									<Label text="{i18n>textContract}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
									<Input placeholder="{i18n>placeEnterContract}" name="HCP_ZCONTRATO" value="{fixedOrderFormModel>/HCP_ZCONTRATO}" type="Tel"
										showValueHelp="false" enabled="{fixedOrderFormModel>/enabled}" required="false" visible="true" width="100%" valueHelpOnly="false"
										maxLength="0" liveChange="_validateForm" change="_validateForm"/>
									<!--Tipo de NF-->
									<Label text="{i18n>textNfType}" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
									<custom:CustomComboBox placeholder="{i18n>placeSelectNfType}" selectedKey="{fixedOrderFormModel>/HCP_ZTPNF}" editable="true"
										enabled="{fixedOrderFormModel>/enabled}" visible="true" required="true" width="auto" valueState="None" maxWidth="100%"
										selectionChange="_validateForm"
										items="{ path: '/View_Domain', filters: {path: 'DOMNAME', operator: 'EQ', value1: 'ZTPNF'} , sorter: { path: 'VALPOS' } }">
										<custom:items>
											<core:Item text="{DDTEXT}" enabled="true" key="{DOMVALUE_L}"/>
										</custom:items>
									</custom:CustomComboBox>
									<!--Observações-->
									<Label text="{i18n>textComments}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
									<TextArea placeholder="{i18n>placeEnterComments}" rows="2" value="{fixedOrderFormModel>/HCP_ZOBS}" liveChange="_validateForm" cols="20"
										maxLength="255" wrapping="None" visible="true" enabled="{fixedOrderFormModel>/enabled}" required="false"/>
									<!--Aprovador-->
									<Label text="{i18n>textApprover}" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
									<custom:CustomComboBox placeholder="{i18n>placeSelectApprover}" selectedKey="{fixedOrderFormModel>/HCP_BEDNR}" editable="true"
										enabled="{fixedOrderFormModel>/enabled}" visible="true" required="true" width="auto" valueState="None" maxWidth="100%"
										selectionChange="_validateForm" items="{ path: 'fixedOrderFormModel>/tableApprover', length: 999999, sorter: { path: 'BEDNR' }}">
										<custom:items>
											<core:Item text="{path:'fixedOrderFormModel>ID_USER'} - {path:'fixedOrderFormModel>NAME_TEXT'}" enabled="true"
												key="{path:'fixedOrderFormModel>ID_USER'}"/>
										</custom:items>
									</custom:CustomComboBox>
									<!-- Tipo de Compra -->
									<Label text="Tipo de Compra" design="Standard" width="100%" required="{fixedOrderFormModel>/errorMaterialDisponible}" textAlign="Begin"
										textDirection="Inherit" visible="{fixedOrderFormModel>/errorMaterialDisponible}"/>
									<custom:CustomComboBox placeholder="{i18n>placeMaterial}" selectedKey="{fixedOrderFormModel>/HCP_ZZTPCOMPRA}"
										enabled="{fixedOrderFormModel>/enabled}" editable="true" visible="{fixedOrderFormModel>/errorMaterialDisponible}" required="true"
										width="auto" valueState="None" maxWidth="100%" selectionChange="_changePurchaseType">
										<custom:items>
											<core:Item text="1 - SPOT" enabled="true" key="1"/>
											<core:Item text="2 - FRAME" enabled="true" key="2"/>
											<core:Item text="3 - TRANSF.SALDO" enabled="true" key="3"/>
										</custom:items>
									</custom:CustomComboBox>
									<!-- Valor liquido do pedido em moeda de pedido -->
									<Label text="CBOT" design="Standard" width="100%" required="{fixedOrderFormModel>/isFrameType}" textAlign="Begin" textDirection="Inherit"
										visible="{fixedOrderFormModel>/isFrameType}"/>
									<Input placeholder="" liveChange="_validateForm" name="HCP_BWERT" change="_validateForm"
										value="{ path: 'fixedOrderFormModel>/HCP_BWERT' , type: 'sap.ui.model.type.Float', formatOptions: { groupingEnabled: true, groupingSeparator: '.', decimalSeparator: ',', maxFractionDigits: 2 }}"
										type="Tel" showValueHelp="false" enabled="{fixedOrderFormModel>/enabled}" required="true" visible="{fixedOrderFormModel>/isFrameType}"
										width="100%" valueHelpOnly="false" maxLength="0"/>
									<!-- Premio -->
									<Label text="Premio" design="Standard" width="100%" required="{fixedOrderFormModel>/isFrameType}" textAlign="Begin" textDirection="Inherit"
										visible="{fixedOrderFormModel>/isFrameType}"/>
									<Input placeholder="" liveChange="_validateForm" name="HCP_ZZPREMIO_CEREAL" change="_validateForm"
										value="{ path: 'fixedOrderFormModel>/HCP_ZZPREMIO_CEREAL' , type: 'sap.ui.model.type.Float', formatOptions: { groupingEnabled: true, groupingSeparator: '.', decimalSeparator: ',', maxFractionDigits: 2 }}"
										type="Tel" showValueHelp="false" enabled="{fixedOrderFormModel>/enabled}" required="true" visible="{fixedOrderFormModel>/isFrameType}"
										width="100%" valueHelpOnly="false" maxLength="0"/>
									<Label text="" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit"
										visible="{fixedOrderFormModel>/isFrameType}"/>
									<!-- Valor liquido do pedido em moeda de pedido -->
									<Label text="Câmbio" design="Standard" width="100%" required="{fixedOrderFormModel>/isFrameType}" textAlign="Begin" textDirection="Inherit"
										visible="{fixedOrderFormModel>/isFrameType}"/>
									<Input placeholder="" liveChange="_validateForm" name="HCP_ZZCAMBIO" change="_validateForm"
										value="{ path: 'fixedOrderFormModel>/HCP_ZZCAMBIO' , type: 'sap.ui.model.type.Float', formatOptions: { groupingEnabled: true, groupingSeparator: '.', decimalSeparator: ',', maxFractionDigits: 2 }}"
										type="Tel" showValueHelp="false" enabled="{fixedOrderFormModel>/enabled}" required="true" visible="{fixedOrderFormModel>/isFrameType}"
										width="100%" valueHelpOnly="false" maxLength="0"/>
								</form:content>
							</form:SimpleForm>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
				<uxap:customAnchorBarButton/>
			</uxap:ObjectPageSection>
		</uxap:sections>
	</uxap:ObjectPageLayout>
