											<!--<NumericContent value="10" valueColor="Good" indicator="None" icon="sap-icon://workflow-tasks"/>-->
											<ImageContent src="" class="mapeamento-oferta"/>
										</content>
									</TileContent>
								</tileContent>
							</GenericTile>
							<GenericTile header="Ficha de visitas" visible="true" subheader="Consultoria" frameType="TwoByOne"
								class="sapUiMediumMarginTopBottom sapUiMediumMarginBeginEnd azul" press="_onSelectVisitForm">
								<tileContent>
									<TileContent class="sapMTitleContentIndex" footer="Agendamentos" unit="">
										<content>
											<!--<NumericContent value="10" valueColor="Good" indicator="None" icon="sap-icon://workflow-tasks"/>-->
											<ImageContent src="" class="ficha"/>
										</content>
									</TileContent>
								</tileContent>
							</GenericTile>
							<GenericTile header="Acompanhamento de Lavoura" visible="true" subheader="Comercial" frameType="TwoByOne"
