<mvc:View xmlns:mvc="sap.ui.core.mvc"
	controllerName="com.sap.build.standard.brfAppDeGraosModoEditar.controller.visitForm.EditYearlyVisitForm" xmlns:uxap="sap.uxap" xmlns="sap.m"
	xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:semantic="sap.m.semantic" xmlns:u="sap.ui.unified"
	xmlns:custom="com.sap.build.standard.brfAppDeGraosModoEditar.controls">
	<uxap:ObjectPageLayout showTitleInHeaderContent="true" showHeaderContent="true" showAnchorBar="true" showFooter="true"
		subSectionLayout="TitleOnTop">
		<uxap:headerTitle>
			<uxap:ObjectPageDynamicHeaderTitle class="displayArrow">
				<uxap:breadcrumbs>
					<OverflowToolbarButton type="Transparent" icon="sap-icon://nav-back" tooltip="Voltar" press="onCancelPress"/>
				</uxap:breadcrumbs>
				<uxap:expandedHeading>
					<FlexBox wrap="Wrap" fitContainer="true" alignItems="Center">
						<Title text="Ficha de Visitas Anual" wrapping="true" class="sapUiTinyMarginEnd sapUiTinyMarginTop"/>
					</FlexBox>
				</uxap:expandedHeading>
				<uxap:expandedContent>
					<FlexBox wrap="Wrap" fitContainer="true" alignItems="Center">
						<Title
							text="{i18n>textVendorProspect}: {editYearlyVisitModel>/HCP_PROVIDER_ID} - {= ${editYearlyVisitModel>/HCP_NAME_REGISTERED} !== null ? ${editYearlyVisitModel>/HCP_NAME_REGISTERED} : ${editYearlyVisitModel>/NAME_LIFNR} !== null ? ${editYearlyVisitModel>/NAME_LIFNR} : ${editYearlyVisitModel>/NAME_PROSPECT}}"
							width="100%" textAlign="Begin" textDirection="Inherit" wrapping="true"/>
					</FlexBox>
				</uxap:expandedContent>
				<uxap:snappedContent>
					<FlexBox wrap="Wrap" fitContainer="true" alignItems="Center">
						<Title
							text="{i18n>textVendorProspect}: {editYearlyVisitModel>/HCP_PROVIDER_ID} - {= ${editYearlyVisitModel>/HCP_NAME_REGISTERED} !== null ? ${editYearlyVisitModel>/HCP_NAME_REGISTERED} : ${editYearlyVisitModel>/NAME_LIFNR} !== null ? ${editYearlyVisitModel>/NAME_LIFNR} : ${editYearlyVisitModel>/NAME_PROSPECT}}"
							width="100%" textAlign="Begin" textDirection="Inherit" wrapping="true"/>
					</FlexBox>
				</uxap:snappedContent>
				<uxap:navigationActions>
					<Button text="Salvar" type="Emphasized" iconFirst="true" width="auto"
						enabled="{= ${editYearlyVisitModel>/enableSave} &amp;&amp; ${profileModel>/edit}}" visible="true" iconDensityAware="false"
						press="onSavePress"/>
				</uxap:navigationActions>
			</uxap:ObjectPageDynamicHeaderTitle>
		</uxap:headerTitle>
		<uxap:sections>
			<uxap:ObjectPageSection showTitle="true" title="Cadastro" titleUppercase="true" visible="true">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="Cadastro" mode="Expanded">
						<uxap:blocks>
							<VBox>
								<form:SimpleForm class="simpleForm" columnsL="1" columnsM="1" columnsXL="1" editable="true" emptySpanL="4" emptySpanM="4"
									id="newEntitySimpleForm" labelSpanL="3" labelSpanM="3" labelSpanXL="3" layout="ResponsiveGridLayout" maxContainerCols="2" minWidth="2048">
									<form:content>
										<Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<form:SimpleForm class="simpleForm" width="100%" columnsL="1" columnsM="1" columnsXL="1" editable="true" emptySpanL="4" emptySpanM="0"
											emptySpanXL="0" id="ufPlantingDataSimpleForm" labelSpanL="3" labelSpanM="3" labelSpanXL="3" layout="ResponsiveGridLayout"
											maxContainerCols="2" minWidth="2048">
											<form:content></form:content>
										</form:SimpleForm>
										<Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"
											visible="{= ${profileModel>/edit} === true ? false : true}"/>
										<MessageStrip text="Você não tem permissão para editar Ficha de Visitas" type="Error" showIcon="true" showCloseButton="false"
											visible="{= ${profileModel>/edit} === true ? false : true}" class="sapUiMediumMarginBottom"/>
										<Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<Button text="Adicionar Novo UF Plantio" type="Emphasized" iconFirst="true" enabled="{editYearlyVisitModel>/isEnable}" visible="true"
											iconDensityAware="false" press="_onAddNewForm">
											<customData>
												<core:CustomData key="id" value="ufPlantingDataSimpleForm"/>
											</customData>
											<layoutData>
												<l:GridData span="L5 M5 S12"/>
											</layoutData>
										</Button>
										<Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<form:SimpleForm class="simpleForm" width="100%" columnsL="1" columnsXL="1" columnsM="1" editable="true" emptySpanL="4" emptySpanM="0"
											emptySpanXL="0" id="newCultureSimpleForm" labelSpanL="3" labelSpanM="3" labelSpanXL="3" layout="ResponsiveGridLayout" maxContainerCols="2"
											minWidth="2048">
											<form:content></form:content>
										</form:SimpleForm>
										<Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<Button class="sapUiLargeMarginBottom" id="cultureFormID" text="Adicionar Nova Cultura" type="Emphasized" iconFirst="true"
											enabled="{editYearlyVisitModel>/isEnable}" visible="true" iconDensityAware="false" press="_onAddNewForm">
											<customData>
												<core:CustomData key="id" value="newCultureSimpleForm"/>
											</customData>
											<layoutData>
												<l:GridData span="L5 M5 S12"/>
											</layoutData>
										</Button>
										<Label text="Tipo de Contato" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<custom:CustomComboBox placeholder="Selecione o Tipo de Contato" selectedKey="{editYearlyVisitModel>/HCP_CONTACT_TYPE}" editable="true"
											enabled="true" visible="true" width="auto" valueState="None" maxWidth="100%" selectionChange="_validateForm">
											<custom:items>
												<core:Item text="Presencial" enabled="true" key="presencial"/>
												<core:Item text="Telefone" enabled="true" key="telefone"/>
												<core:Item text="Mensagem via Telefone" enabled="true" key="mensagemtelefone"/>
												<core:Item text="Skype" enabled="true" key="skype"/>
												<core:Item text="E-mail" enabled="true" key="email"/>
												<core:Item text="Outros" enabled="true" key="outros"/>
											</custom:items>
										</custom:CustomComboBox>
										<Label text="Possui Estrutura de Armazenagem?" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit"
											visible="true"/>
										<SegmentedButton width="100%" class="sapUiSmallMarginBottom" selectedKey="{editYearlyVisitModel>/HCP_STORAGE_STRUCTURE}"
											selectionChange="_onInputStorageStructure" enabled="{editYearlyVisitModel>/isEnable}">
											<items>
												<SegmentedButtonItem text="Sim" key="1"/>
												<SegmentedButtonItem text="Não" key="0"/>
											</items>
										</SegmentedButton>
										<Label text="Tipo de Armazenagem" design="Standard" width="100%" required="{editYearlyVisitModel>/yesStorage}" textAlign="Begin"
											textDirection="Inherit" visible="true"/>
										<MultiComboBox
											items="{ path: '/Storage_Type', length: 999999 , filters: [{path: 'HCP_LAND1', operator: 'EQ', value1: 'BR'}, {path: 'HCP_ACTIVE', operator: 'EQ', value1: '1'}], sorter: { path: 'HCP_ID',descending: false}}"
											placeholder="Selecione o Tipo de Armazenagem" required="{editYearlyVisitModel>/yesStorage}" editable="true"
											selectedKeys="{editYearlyVisitModel>/negotiationInputs}" enabled="{editYearlyVisitModel>/isEnable}" valueState="None"
											visible="{editYearlyVisitModel>/yesStorage}" change="_validateForm" selectionChange="_validateForm">
											<items>
												<core:Item text="{HCP_DESC}" enabled="true" key="{HCP_ID}"/>
											</items>
										</MultiComboBox>
										<Label text="{i18n>textWhatsCapacity}" design="Standard" width="100%" required="{editYearlyVisitModel>/yesStorage}" textAlign="Begin"
											textDirection="Inherit" visible="true"/>
										<Input placeholder="{i18n>textWhatsCapacityField}" name="HCP_CAPACITY"
											value="{ path: 'editYearlyVisitModel>/HCP_CAPACITY' , type: 'sap.ui.model.type.Float', formatOptions: { groupingEnabled: true, groupingSeparator: '.', decimalSeparator: ',', maxFractionDigits: 2 }}"
											type="Tel" showValueHelp="false" enabled="true" required="{editYearlyVisitModel>/yesStorage}" visible="{editYearlyVisitModel>/yesStorage}"
											width="100%" valueHelpOnly="false" maxLength="0" liveChange="_valideInputNumber" change="_valideInputNumber"/>
										<Label text="{i18n>textWhatsSilosBag}" design="Standard" width="100%" required="{editYearlyVisitModel>/yesStorage}" textAlign="Begin"
											textDirection="Inherit" visible="{editYearlyVisitModel>/yesStorage}"/>
										<SegmentedButton width="100%" class="sapUiSmallMarginBottom" selectedKey="{editYearlyVisitModel>/HCP_SILOS_BAG}"
											selectionChange="_onInputSilosStructure" visible="{editYearlyVisitModel>/yesStorage}">
											<items>
												<SegmentedButtonItem text="{i18n>textYes}" key="1"/>
												<SegmentedButtonItem text="{i18n>textNo}" key="0"/>
											</items>
										</SegmentedButton>
										<Label text="{i18n>textWhatsSilosVolume}" design="Standard" width="100%" required="{editYearlyVisitModel>/yesSilosBag}" textAlign="Begin"
											textDirection="Inherit" visible="{editYearlyVisitModel>/yesSilosBag}"/>
										<Input placeholder="{i18n>textWhatsSilosVolumeField}" name="HCP_SILO_VOLUME"
											value="{ path: 'editYearlyVisitModel>/HCP_SILO_VOLUME' , type: 'sap.ui.model.type.Float', formatOptions: { groupingEnabled: true, groupingSeparator: '.', decimalSeparator: ',', maxFractionDigits: 2 }}"
											type="Tel" showValueHelp="false" enabled="true" required="{editYearlyVisitModel>/yesSilosBag}" visible="{editYearlyVisitModel>/yesSilosBag}"
											width="100%" valueHelpOnly="false" maxLength="0" liveChange="_valideInputNumber" change="_valideInputNumber"/>
										<Label text="Realiza Barter/Troca?" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<SegmentedButton width="100%" class="sapUiSmallMarginBottom" enabled="{editYearlyVisitModel>/isEnable}"
											selectedKey="{editYearlyVisitModel>/HCP_BARTER_EXCHANGE}" selectionChange="_onInputBarterExchange">
											<items>
												<SegmentedButtonItem text="Sim" key="1"/>
												<SegmentedButtonItem text="Não" key="0"/>
											</items>
										</SegmentedButton>
										<Label text="Quantidade % " design="Standard" width="100%" required="{editYearlyVisitModel>/yesBarter}" textAlign="Begin"
											textDirection="Inherit" visible="{editYearlyVisitModel>/yesBarter}"/>
										<custom:CustomSlider min="0" max="100" value="{editYearlyVisitModel>/HCP_AMOUNT}" step="1" enabled="{editYearlyVisitModel>/isEnable}"
											visible="{editYearlyVisitModel>/yesBarter}" width="100%" progress="true" change="_validateForm" showAdvancedTooltip="false">
											<custom:layoutData>
												<l:GridData span="L4 M4 S9"/>
											</custom:layoutData>
										</custom:CustomSlider>
										<NumericContent animateTextChange="true" value="{editYearlyVisitModel>/HCP_AMOUNT}" width="" scale="%" class="percentageIndicator"
											withMargin="true" visible="{editYearlyVisitModel>/yesBarter}">
											<layoutData>
												<l:GridData span="L1 M1 S3"/>
											</layoutData>
										</NumericContent>
										<Label text="Certificações?" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<MultiComboBox
											items="{ path: '/Visit_Certifications', length: 999999 , filters: [{path: 'HCP_LAND1', operator: 'EQ', value1: 'BR'}, {path: 'HCP_ACTIVE', operator: 'EQ', value1: '1'} ], sorter: { path: 'HCP_ID',descending: false}}"
											placeholder="Selecione as Certificações" required="false" editable="true" selectedKeys="{editYearlyVisitModel>/certifications}"
											enabled="{editYearlyVisitModel>/isEnable}" valueState="None" selectionChange="_validateInputCertification">
											<items>
												<core:Item text="{HCP_DESC}" enabled="true" key="{HCP_ID}"/>
											</items>
										</MultiComboBox>
										<Label text="{i18n>textOthers}" design="Standard" width="100%" required="{editYearlyVisitModel>/yesOthers}" textAlign="Begin"
											textDirection="Inherit" visible="true"/>
										<TextArea rows="2" placeholder="Descritivo para outros" visible="{editYearlyVisitModel>/yesOthers}"
											value="{editYearlyVisitModel>/HCP_OTHERS}" cols="20" maxLength="255" wrapping="None" enabled="{editYearlyVisitModel>/isEnable}"
											required="{editYearlyVisitModel>/yesOthers}" liveChange="_validateForm"/>
										<Label text="Indique o Principal Parceiro de Insumos" design="Standard" width="100%" required="true" textAlign="Begin"
											textDirection="Inherit" visible="true"/>
										<SegmentedButton selectedKey="{editYearlyVisitModel>/partnerProspect}" width="100%" class="sapUiSmallMarginBottom"
											selectionChange="_onInputFormSelect" enabled="{editYearlyVisitModel>/isEnable}">
											<items>
												<SegmentedButtonItem text="Fornecedor" key="Fornecedor"/>
												<SegmentedButtonItem text="Prospect" key="Prospect"/>
											</items>
										</SegmentedButton>
										<Label text="Base de Fornecedores BRF?" design="Standard" width="100%" required="{editYearlyVisitModel>/yesPartnerBRF}" textAlign="Begin"
											textDirection="Inherit" visible="{editYearlyVisitModel>/yesPartnerBRF}"/>
										<SegmentedButton width="100%" class="sapUiSmallMarginBottom" selectedKey="{editYearlyVisitModel>/HCP_PARTNER_BRF_SUP}"
											selectionChange="_onInputPartnerFormSelect" enabled="{editYearlyVisitModel>/isEnable}" visible="{editYearlyVisitModel>/yesPartnerBRF}">
											<items>
												<SegmentedButtonItem text="Sim" key="1"/>
												<SegmentedButtonItem text="Não" key="0"/>
											</items>
										</SegmentedButton>
										<Label text="Parceiro" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<Input id="yesPartner" visible="{editYearlyVisitModel>/yesPartner}" required="{editYearlyVisitModel>/yesPartner}" liveChange="_validateForm"
											change="_validateForm" editable="true" enabled="{editYearlyVisitModel>/isEnable}" placeholder="Selecione o Parceiro" showValueHelp="true"
											type="Text" value="{editYearlyVisitModel>/HCP_PARTNER}" description="{editYearlyVisitModel>/partnerDesc}" valueHelpOnly="true"
											valueHelpRequest="_handlePartnerFilterPress"/>
										<Label text="Parceiro" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										<Input id="noPartner" placeholder="Digite o Parceiro" type="Text" value="{editYearlyVisitModel>/HCP_PARTNER}" showValueHelp="false"
											required="false" enabled="{editYearlyVisitModel>/isEnable}" visible="{editYearlyVisitModel>/noPartner}" width="100%"
											liveChange="_validateForm" change="_validateForm" valueHelpOnly="false" maxLength="0"/>
										<Label text="Prospect" design="Standard" width="100%" required="{editYearlyVisitModel>/yesProspect}" textAlign="Begin"
											textDirection="Inherit" visible="{editYearlyVisitModel>/yesProspect}"/>
										<custom:CustomComboBox id="yesProspect" placeholder="Selecione o Prospect" selectedKey="{editYearlyVisitModel>/HCP_PARTNER}" editable="true"
											enabled="{editYearlyVisitModel>/isEnable}" visible="{editYearlyVisitModel>/yesProspect}" required="{editYearlyVisitModel>/yesProspect}"
											width="auto" valueState="None" maxWidth="100%" selectionChange="_validateForm"
											items="{ path: '/Prospects', length: 999999, filters: [{path: 'HCP_STATUS', operator: 'EQ', value1: '1'}, {path: 'HCP_STATUS', operator: 'EQ', value1: '2'}] }">
											<custom:items>
												<core:Item text="{NAME1} {NAME2}" enabled="true" key="{HCP_PROSP_ID}"/>
											</custom:items>
										</custom:CustomComboBox>
									</form:content>
								</form:SimpleForm>
							</VBox>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
				<uxap:customAnchorBarButton/>
			</uxap:ObjectPageSection>
		</uxap:sections>
	</uxap:ObjectPageLayout>
</mvc:View>