							<GenericTile id="agendaHiddenID" header="Gestão de Agenda" subheader="Comercial" visible="true" frameType="TwoByOne"
								class="sapUiMediumMarginTopBottom sapUiMediumMarginBeginEnd azul" press="_onAppointmentsTilePress">
								<tileContent>
									<TileContent class="sapMTitleContentIndex" footer="Agendamentos" unit="">
										<content>
											<!--<NumericContent value="10" valueColor="Good" indicator="None" icon="sap-icon://workflow-tasks"/>-->
											<ImageContent src="" class="gestao"/>
										</content>
									</TileContent>
								</tileContent>
							</GenericTile>
								<GenericTile header="Tabela de Preços" visible="true" subheader="Comercial" frameType="TwoByOne"
