<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	controllerName="com.sap.build.standard.brfAppDeGraosModoEditar.controller.Index" xmlns:microchart="sap.suite.ui.microchart" xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:form="sap.ui.layout.form">
	<App id="myApp">
		<pages>
			<Page showHeader="false" title="BRF App Grãos" showFooter="false" showNavButton="false" busy="{indexModel>/keepBusy}"
				visible="{indexModel>/pageLoad}">
				<subHeader>
					<Toolbar class="headerIndex">
						<ToolbarSpacer/>
						<Title text="AMB.TESTE {indexModel>/appVersion}" level="H3" class="titleIndexMargin"/>
						<ToolbarSpacer/>
						<Button type="Emphasized" icon="sap-icon://synchronize" press="refreshData" visible="{indexModel>/isMobile}"/>
						<Button icon="sap-icon://message-popup" text="{indexModel>/messageCounter}" type="Emphasized" press="showMessageDialog"
							busy="{indexModel>/messageBusyIndicator}"/>
						<!--<Button type="Emphasized" text="Logout" press="logoff" visible="false"/>-->
						<!--<Switch state="{indexModel>/switch}" change="enablePhoto" customTextOn="On" customTextOff="Off">-->
						<!--	<layoutData>-->
						<!--		<FlexItemData growFactor="1" />-->
						<!--	</layoutData>-->
						<!--</Switch>-->
					</Toolbar>
				</subHeader>
				<content>
					<VBox visible="{photoModel>/hasOfflinePhotos}">
						<MessageStrip class="sapUiTinyMargin"
							text="Você tem {= ${photoModel>/ItemPhoto}.length} {= ${photoModel>/ItemPhoto}.length > 1 ? 'Fotos' : 'Foto'} Offline. Deseja realizar o upload?"
							type="Success" showIcon="true"></MessageStrip>
						<OverflowToolbar>
							<ToolbarSpacer/>
							<Button text="Upload" type="Emphasized" press="_base64ToImage"/>
						</OverflowToolbar>
					</VBox>
					<VBox class="background-index">
						<MessageStrip text="Ultima atualização base ECC {indexModel>/lastUpdate}, clique para atualizar." showIcon="true" showCloseButton="false"
							visible="{indexModel>/isMobile}" class="sapUiSmallMargin"></MessageStrip>
						<MessageStrip visible="{indexModel>/hasCountOfferMap}" class="sapUiTinyMargin" text="{indexModel>/countStorageOfferMap}" type="Warning"
							showCloseButton="true">
							<link>
								<Link text="Atualizar Ofertas." press="refreshOfferMapData"/>
							</link>
						</MessageStrip>
						<MessageStrip visible="{indexModel>/hasCountCommodities}" class="sapUiTinyMargin" text="{indexModel>/countStorageCommodities}"
							type="Warning" showCloseButton="true">
							<link>
								<Link text="Atualizar Compras." press="refreshCommoditiesData"/>
							</link>
						</MessageStrip>
							<MessageStrip type="Error" visible="{indexModel>/hasPendingCommodities}" class="sapUiTinyMargin" text="Você possui {indexModel>/countPendingCommodities} compras fixas pendentes de envio ao SAP."
							showCloseButton="true">
							<link>
								<Link text="Verificar." press="goToPendingCommodities"/>
							</link>
						</MessageStrip>
							<MessageStrip type="Error" visible="{indexModel>/hasPendingDepositCommodities}" class="sapUiTinyMargin" text="Você possui {indexModel>/countPendingDepositCommodities} compras depósito pendentes de envio ao SAP."
							showCloseButton="true">
							<link>
								<Link text="Verificar." press="goToPendingDepositCommodities"/>
							</link>
						</MessageStrip>
						<VBox id="headerID">
							<Panel id="panelID" width="auto" expandable="true" expanded="{indexModel>/expandIndex}" visible="true" headerText="Seu Extrato"
								class="extractPainel extrato-compras sapUiTinyMarginTopBottom ">
								<GenericTile header="Volume de compras" press="_onPriceIntentionTile" subheader="" frameType="OneByOne"
									class="sapUiSmallMarginBottom sapUiTinyMarginBegin tileStyle2">
									<tileContent>
										<TileContent footer="Toneladas" unit="">
											<content>
												<microchart:ComparisonMicroChart size="S" scale="" view="" shrinkable="false" width="auto">
													<microchart:data>
														<microchart:ComparisonMicroChartData value="100" color="Good" title="MÊS VIGENTE"
															displayValue="{ path: 'indexModel>/totalCurrentPurchaseVolume' , type: 'sap.ui.model.type.Float', formatOptions: { groupingEnabled: true, groupingSeparator: '.', decimalSeparator: ',', maxFractionDigits: 2 }}"/>
														<microchart:ComparisonMicroChartData value="0" color="Neutral" title="Mês Anterior"
															displayValue="{ path: 'indexModel>/totalLastMonthPurchaseVolume' , type: 'sap.ui.model.type.Float', formatOptions: { groupingEnabled: true, groupingSeparator: '.', decimalSeparator: ',', maxFractionDigits: 2 }}"/>
													</microchart:data>
												</microchart:ComparisonMicroChart>
											</content>
										</TileContent>
									</tileContent>
								</GenericTile>
								<GenericTile header="Nº de Compromissos Realizados" press="_onPriceIntentionTile" subheader="" frameType="OneByOne"
									class="sapUiSmallMarginBottom sapUiTinyMarginBegin tileStyle2">
									<tileContent>
										<TileContent footer="Visitas" unit="">
											<content>
												<microchart:ComparisonMicroChart size="S" scale="" view="" shrinkable="false" width="auto">
													<microchart:data>
														<microchart:ComparisonMicroChartData value="100" color="Critical" title="MÊS VIGENTE" displayValue="{indexModel>/totalAppointment}"/>
														<microchart:ComparisonMicroChartData value="0" color="Neutral" title="Mês Anterior" displayValue="{indexModel>/totalAppointmentLM}"/>
													</microchart:data>
												</microchart:ComparisonMicroChart>
											</content>
										</TileContent>
									</tileContent>
								</GenericTile>
								<GenericTile header="Nº de Ofertas em Aberto" press="_onPriceIntentionTile" subheader="" frameType="OneByOne"
									class="sapUiSmallMarginBottom sapUiTinyMarginBegin tileStyle2">
									<tileContent>
										<TileContent footer="Ofertas" unit="">
											<content>
												<microchart:ComparisonMicroChart size="S" scale="" view="" shrinkable="false" width="auto">
													<microchart:data>
														<microchart:ComparisonMicroChartData value="0" color="Neutral" title="MÊS VIGENTE" displayValue="{indexModel>/totalOffers}"/>
														<microchart:ComparisonMicroChartData value="0" color="Neutral" title="Mês Anterior" displayValue="{indexModel>/totalOffersLM}"/>
													</microchart:data>
												</microchart:ComparisonMicroChart>
											</content>
										</TileContent>
									</tileContent>
								</GenericTile>
								<GenericTile header="Vol. de Ofertas em Aberto" press="_onPriceIntentionTile" subheader="" frameType="OneByOne"
									class="sapUiSmallMarginBottom sapUiTinyMarginBegin tileStyle2">
									<tileContent>
										<TileContent footer="Toneladas" unit="">
											<content>
												<microchart:ComparisonMicroChart size="S" scale="" view="" shrinkable="false" width="auto">
													<microchart:data>
														<microchart:ComparisonMicroChartData value="0" color="Neutral" title="MÊS VIGENTE"
															displayValue="{ path: 'indexModel>/totalVolume' , type: 'sap.ui.model.type.Float', formatOptions: { groupingEnabled: true, groupingSeparator: '.', decimalSeparator: ',', maxFractionDigits: 2 }}"/>
														<microchart:ComparisonMicroChartData value="0" color="Neutral" title="Mês Anterior"
															displayValue="{ path: 'indexModel>/totalVolumeLM' , type: 'sap.ui.model.type.Float', formatOptions: { groupingEnabled: true, groupingSeparator: '.', decimalSeparator: ',', maxFractionDigits: 2 }}"/>
													</microchart:data>
												</microchart:ComparisonMicroChart>
											</content>
										</TileContent>
									</tileContent>
								</GenericTile>
							</Panel>
						</VBox>
						<Title class="titulo-grupo" visible="true" text="Menu" level="H2"/>
