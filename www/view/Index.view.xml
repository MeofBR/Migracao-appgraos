								<tileContent>
									<TileContent footer="Agendamentos" unit="">
										<content>
											<ImageContent src="" class="tabelaDePrecos"/>
										</content>
									</TileContent>
								</tileContent>
							</GenericTile>
							<GenericTile header="{i18n>textOfferMap}" visible="true" subheader="Comercial" frameType="TwoByOne"
								class="sapUiMediumMarginTopBottom sapUiMediumMarginBeginEnd azul" press="_onOfferMapPress">
