<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout"
	controllerName="com.sap.build.standard.brfAppDeGraosModoEditar.controller.visitForm.Edit" xmlns="sap.m" xmlns:core="sap.ui.core">
	<Page title="Consulta Ficha de Visitas" showNavButton="true" navButtonPress="onCancelPress" showFooter="true">
		<headerContent>
			<Button icon="sap-icon://home" tooltip="Home" press="_onHome" visible="true"/>
		</headerContent>
		<content>
			<!--<PullToRefresh id="pullToRefreshID" visible="{device>/support/touch}" refresh="refreshData"/>-->
			<IconTabBar id="idIconTabBar" select="handleIconTabBarSelect" expanded="true" expandable="false" class="sapUiResponsiveContentPadding">
				<items>
					<IconTabFilter icon="sap-icon://appointment-2" iconColor="Default" design="Horizontal" text="Anual" key="Yearly">
						<core:Fragment fragmentName="com.sap.build.standard.brfAppDeGraosModoEditar.view.visitForm.fragments.TableViewYearly" type="XML"/>
					</IconTabFilter>
					<IconTabFilter icon="sap-icon://appointment" iconColor="Critical" design="Horizontal" text="Periódico" key="Periodic">
						<core:Fragment fragmentName="com.sap.build.standard.brfAppDeGraosModoEditar.view.visitForm.fragments.TableViewPeriodic" type="XML"/>
					</IconTabFilter>
					<IconTabFilter icon="sap-icon://e-care" iconColor="Positive" design="Horizontal" text="Grãos" key="Grains">
						<core:Fragment fragmentName="com.sap.build.standard.brfAppDeGraosModoEditar.view.visitForm.fragments.TableViewGrains" type="XML"/>
					</IconTabFilter>
					<IconTabFilter icon="sap-icon://factory" iconColor="Neutral" design="Horizontal" text="Indústria" key="Industry">
						<core:Fragment fragmentName="com.sap.build.standard.brfAppDeGraosModoEditar.view.visitForm.fragments.TableViewIndustry" type="XML"/>
					</IconTabFilter>
					<IconTabFilter icon="sap-icon://product" iconColor="Negative" design="Horizontal" text="Comercialização" key="Commercialization" visible="true">
						<core:Fragment fragmentName="com.sap.build.standard.brfAppDeGraosModoEditar.view.visitForm.fragments.CommercializationView" type="XML"/>
					</IconTabFilter>
				</items>
			</IconTabBar>
		</content>
	</Page>
</mvc:View>