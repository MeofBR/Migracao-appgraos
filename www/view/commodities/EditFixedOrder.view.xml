									<DatePicker id="idDateInput1" placeholder="{i18n>placeSelectPaymentDate1}" displayFormat="short" name="HCP_ZDTPGTO1" enabled="{fixedOrderFormModel>/enabled}"
										value="{ path: 'fixedOrderFormModel>/HCP_ZDTPGTO1', type: 'sap.ui.model.type.Date', formatOptions: { style: 'short', UTC: false, pattern: 'dd/MM/yyyy' } }"
										dateValue="" change="_onInputPaymentDate1FormSelect" visible="{fixedOrderFormModel>/visibleFixedDate}"/>
									<!--Data Pagamento 2-->
									<Label text="{i18n>textPaymentDate} 2" design="Standard" required="false" textAlign="Begin" textDirection="Inherit" visible="{fixedOrderFormModel>/visibleFixedDate}"/>
									<DatePicker id="idDateInput2" placeholder="{i18n>placeSelectPaymentDate1}" displayFormat="short" name="HCP_ZDTPGTO2" enabled="{fixedOrderFormModel>/enabled}"
										value="{ path: 'fixedOrderFormModel>/HCP_ZDTPGTO2', type: 'sap.ui.model.type.Date', formatOptions: { style: 'short', UTC: false, pattern: 'dd/MM/yyyy' } }"
